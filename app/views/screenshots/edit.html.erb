<h1>Edit Screenshot</h1>
<%= link_to('Add another to this session', new_session_screenshot_path(@session)) %>
<%= form_for(@screenshot, :url => session_screenshot_path(@session, @screenshot), :html => { :multipart => true }) do |f| %>
  <%= render(:partial => 'form', :locals => { :f => f }) %>
<% end %>

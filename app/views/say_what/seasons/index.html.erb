<h1>Listing Seasons</h1>

<%= link_to('New Season', new_say_what_season_path) %>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<% @seasons.each do |season| %>
    <tr>
      <td><%= link_to(season.full_name, say_what_season_path(season)) %></td>
      <td><%= link_to('Edit', edit_say_what_season_path(season)) %> | <%= link_to('Destroy', say_what_season_path(season), :method => :delete, :data => { :confirm => 'Are you sure?' }) %></td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link_to('New Season', new_say_what_season_path) %>
